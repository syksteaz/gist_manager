<div class="container">
  <div class="text-center padded">
    <h1>GistMeUp!</h1>
    <h4>Find the perfect Gist</h4>
    <input type="text" id="username" placeholder="username">
    <button class="btn btn-sm btn-primary" id="search">
      Search
    </button>
  </div>

  <div id="gists">
    <% @gists.each do |gist| %>
      <%= render 'shared/card', gist: gist %>
    <% end %>
  </div>

</div>
